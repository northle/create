{
  "name": "norther-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/main",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "db:migrate": "norther db:migrate",
    "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint .",
    "start": "norther start:dev",
    "start:dev": "norther start:dev",
    "start:prod": "norther start:prod",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/coverage-c8": "^0.23.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.23.0",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "typescript": "^4.8.2",
    "vitest": "^0.23.0"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1"
  },
  "prisma": {
    "schema": "database/schema.prisma"
  }
}
